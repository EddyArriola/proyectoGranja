
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model inventario{
  InventarioID Int @id @default(autoincrement())
    FechaIngreso String
    FechaVenta String
    CantidadIngreso Int
    CantidadVenta Int
	Existencias Int
}

model Cliente{
  ClienteID Int @id @default(autoincrement())
  Nombre String
  Direccion String
}

